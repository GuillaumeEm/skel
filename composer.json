{
    "name": "guillaumeem/skel",
    "type": "template",
    "description": "A template for php thinkering stuff",
    "keywords": [
        "GuillaumeEm",
        "skeleton"
    ],
    "homepage": "https://github.com/GuillaumeEm/skel",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Moal",
            "email": "guillaume-7a08f8978aad@nuaj.eu",
            "homepage": "https://nuaj.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.3",
        "league/climate": "^3.8",
        "league/container": "^4.2",
        "league/flysystem": "^3.0",
        "league/iso3166": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=11.0",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs App tests",
        "fix-style": "phpcbf App tests"
    },
    "config": {
        "sort-packages": true
    },
    "bin":["bin/console"]
}
